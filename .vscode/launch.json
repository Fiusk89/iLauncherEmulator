    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/iLEmu-system-x86_64",
            "args": [
                "-bios",
                "${workspaceFolder}/bios/bios.bin",
                "-memory",
                "16"
